{
  "name": "@ryxon/utils",
  "version": "1.4.2",
  "description": "Ryxon Composition utils",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rslib build -w",
    "build": "rslib build",
    "release": "pnpm build && release-it",
    "prepare": "pnpm build"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterPanY/ryxon.git",
    "directory": "packages/ryxon-utils"
  },
  "bugs": "https://github.com/PeterPanY/ryxon/issues",
  "author": "PeterPanY",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@ryxon/use": "^1.2.1",
    "@types/lodash": "^4.17.16",
    "@types/lodash-es": "^4.17.12",
    "@vueuse/core": "^10.7.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3"
  },
  "devDependencies": {
    "@rslib/core": "^0.6.3",
    "release-it": "^17.10.0",
    "type-fest": "^2.14.0",
    "typescript": "^5.4.5",
    "vue": "^3.4.26"
  },
  "release-it": {
    "git": {
      "tag": false,
      "commitMessage": "release: @ryxon/utils v${version}"
    }
  }
}

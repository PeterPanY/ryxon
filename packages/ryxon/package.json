{
  "name": "ryxon",
  "version": "1.1.7",
  "description": "Components built on Vue",
  "main": "lib/ryxon.cjs.js",
  "module": "es/index.mjs",
  "style": "lib/index.css",
  "typings": "lib/index.d.ts",
  "unpkg": "lib/ryxon.min.js",
  "jsdelivr": "lib/ryxon.min.js",
  "files": [
    "es",
    "lib"
  ],
  "scripts": {
    "dev": "ryxon-cli dev",
    "lint": "ryxon-cli lint",
    "test": "ryxon-cli test",
    "build": "ryxon-cli build",
    "build:site": "ryxon-cli build-site",
    "release": "cp ../../README.md ./ && ryxon-cli release && rm ./README.md",
    "release:site": "pnpm build:site && npx gh-pages -d site-dist --add",
    "test:update": "ryxon-cli test --updateSnapshot",
    "test:watch": "ryxon-cli test --watch",
    "test:coverage": "open test/coverage/index.html"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterPanY/ryxon.git",
    "directory": "packages/ryxon"
  },
  "bugs": "https://github.com/PeterPanY/ryxon/issues",
  "author": "PeterPanY",
  "license": "MIT",
  "keywords": [
    "ui",
    "vue",
    "vue3",
    "frontend",
    "component",
    "components"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.6.0",
    "@ryxon/popperjs": "^1.0.3",
    "@ryxon/use": "^1.0.4",
    "@types/lodash": "^4.14.182",
    "@types/lodash-es": "^4.17.6",
    "@vueuse/core": "^9.1.0",
    "dayjs": "^1.11.7",
    "escape-html": "^1.0.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.2",
    "normalize-wheel-es": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/jest": "^29.1.1",
    "@ryxon/area-data": "workspace:*",
    "@ryxon/cli": "workspace:*",
    "@ryxon/eslint-config": "workspace:*",
    "@ryxon/icons": "workspace:*",
    "@vue/runtime-core": "^3.2.47",
    "@vue/test-utils": "^2.0.2",
    "type-fest": "^2.14.0",
    "typescript": "^5.0.2",
    "vue": "^3.2.47",
    "vue-router": "^4.1.5"
  },
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.css"
  ],
  "release-it": {
    "git": {
      "commitMessage": "release: ryxon v${version}"
    }
  },
  "web-types": "lib/web-types.json"
}

{
  "name": "@ryxon/icons",
  "version": "1.0.3",
  "description": "ryxon icons",
  "type": "module",
  "keywords": [
    "icon",
    "ryxon"
  ],
  "license": "MIT",
  "bugs": "https://github.com/PeterPanY/ryxon/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterPanY/ryxon.git",
    "directory": "packages/ryxon-icons"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "unpkg": "dist/index.iife.min.js",
  "jsdelivr": "dist/index.iife.min.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "pnpm clean && pnpm run build:generate && run-p build:build build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "release": "release-it",
    "prepare": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "PeterPanY",
  "devDependencies": {
    "@ryxon/icons-svg": "workspace:*",
    "@pnpm/find-workspace-dir": "^4.0.3",
    "@pnpm/find-workspace-packages": "^4.0.41",
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.8.3",
    "@types/prettier": "^2.7.1",
    "camelcase": "^7.0.0",
    "chalk": "^5.1.0",
    "consola": "^2.15.3",
    "esbuild": "^0.15.10",
    "esbuild-plugin-globals": "^0.1.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "release-it": "^15.4.1",
    "tsx": "^3.6.0",
    "typescript": "^4.8.4",
    "unplugin-vue": "^3.1.3",
    "vue": "^3.2.40",
    "vue-tsc": "^1.0.0"
  },
  "release-it": {
    "git": {
      "tag": false,
      "commitMessage": "release: @ryxon/icons ${version}"
    }
  }
}

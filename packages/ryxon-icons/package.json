{
  "name": "@ryxon/icons",
  "version": "1.2.0",
  "description": "ryxon icons",
  "type": "module",
  "keywords": [
    "icon",
    "svg",
    "vue",
    "ryxon"
  ],
  "license": "MIT",
  "bugs": "https://github.com/PeterPanY/ryxon/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterPanY/ryxon.git",
    "directory": "packages/ryxon-icons"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "types": "./dist/types/global.d.ts",
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./dist/types/*"
      ]
    }
  },
  "unpkg": "dist/index.iife.min.js",
  "jsdelivr": "dist/index.iife.min.js",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "pnpm clean && pnpm run build:generate && run-p build:build build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "release": "release-it",
    "prepare": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "author": "PeterPanY",
  "devDependencies": {
    "@ryxon/icons-svg": "workspace:*",
    "@pnpm/find-workspace-dir": "^6.0.2",
    "@pnpm/find-workspace-packages": "^6.0.9",
    "@pnpm/logger": "^5.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.3.1",
    "@types/prettier": "^2.7.3",
    "camelcase": "^7.0.1",
    "chalk": "^5.2.0",
    "consola": "^3.1.0",
    "esbuild": "^0.18.4",
    "esbuild-plugin-globals": "^0.2.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "release-it": "^16.1.3",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "unplugin-vue": "^4.2.1",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.0"
  },
  "release-it": {
    "git": {
      "tag": false,
      "commitMessage": "release: @ryxon/icons v${version}"
    }
  }
}
